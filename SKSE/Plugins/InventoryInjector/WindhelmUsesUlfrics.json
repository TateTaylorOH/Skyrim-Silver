{
  "$schema": "https://raw.githubusercontent.com/Exit-9B/InventoryInjector/main/docs/InventoryInjector.schema.json",
  "rules": [
    {
      "match": {
        "formType": "MiscItem",
        "formId": {
          "anyOf": [
            "Update.esm|DE5024"
          ]
        }
      },
      "assign": {
        "subType": "Gold"
      }
    },
    {
      "match": {
        "formType": "MiscItem",
        "subType": "Gold"
      },
      "assign": {
        "subTypeDisplay": "$Currency"
      }
    }
  ]
}